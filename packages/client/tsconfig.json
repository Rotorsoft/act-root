{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "strict": true,
    "outDir": "dist",
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "paths": {
      "@rotorsoft/*": [
        "../../libs/*/src"
      ]
    },
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "../../libs/act-examples",
    },
    {
      "path": "../../libs/act",
    }
  ]
}